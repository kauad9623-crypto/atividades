#include <stdio.h>

int main {
    double largura, comprimento, preco_m2;
    double area, preco_total;
    printf("Digite a largura do terreno: ");
scanf("%lf", &largura);
printf("Digite o comprimento do terreno: ");
scanf("%lf", &comprimento);
printf("Digite o preco por metro quadrado: ");
scanf("%lf", &preco_m2);
area = largura * comprimento;
preco_total = area * preco_m2;
printf("Area do terreno = %.2lf\n", area);
printf("Preco do terreno = %.2lf\n", preco_total);
return 0;
}
